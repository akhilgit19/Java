LOGICAL OPERATORS
====================
package com.operators.flm;

public class booleanOperators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		boolean b1=true;
		boolean b2=true;
		
		System.out.println(b1 || b2);
		
		System.out.println(!b1);
		
		System.out.println(!(b1 || b2));
		System.out.println(!((b1 && b2) || (b2 || b1)));
		
	}
}
