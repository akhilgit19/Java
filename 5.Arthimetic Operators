
Arthimetic Operators:
======================

package com.operators.flm;

import java.util.Scanner;

public class operatorArthimeticOperator {

	//Arthimetic
	//Relational
	//Logical
	//comparision operators
	// assignment opeerators
	
	
	public static void main(String[] args) {
		
		  
//		  Scanner scannerObject = new Scanner(System.in);
//		  
//		  int a=10;
//		  int b=20;
//		  System.out.println(" Before swapping values are");
//		  System.out.println(" A value is "+a);
//		  System.out.println(" B value is "+b);
//		  
//		  a=a+b;
//		  b=a-b;
//		  a=a-b;
//		  
//		  System.out.println(" After swapping values are");
//		  System.out.println(" A value is "+a);
//		  System.out.println(" B value is "+b);
		  
		arthOperators();
		
		}

		private static void swappingOfTwoVariables() {
			Scanner scannerObject = new Scanner(System.in);
			  int a=10;
			  int b=20;
			  
			//  a+=b;
			//  a-=b;
			//  a*=b;
			  
			  int c=a;
			  
			  a=b;
			  b=c;
			  
			  System.out.println("A value is "+a);
			  System.out.println("B value is "+b);
			  System.out.println("c value is "+c);
		}

		private static void arthOperators() {
			// TODO Auto-generated method stub
			  // + - * / % ARTHIMETIC OPERATORS
	      
		  Scanner scannerObject=new Scanner(System.in); 
	      System.out.println("Please enter first value");
	      int a=scannerObject.nextInt();
	      System.out.println("Please enter second value");
	      int b=scannerObject.nextInt();
	       
		
	      int sum=a+b;
	       
	      System.out.println("Sum of two numbers   "+sum);
	      System.out.println("Subraction  "+(a-b));
	      System.out.println("Multiplication "+(a*b));
	      System.out.println("Division  "+(a/b));
	      System.out.println("Reminder  "+(a%b));
		}
}
